/*
Given an array of integers as strings and numbers, return the sum of the array values as if all were numbers.

Return your answer as a number.
*/

// sum array using arr.reduce() and change all values to Numbers using unary + operator (+current)
let sumMix = x => x.reduce((sum, current) => sum + +current, 0)

let arr = ['2', 6, '10', 12]
sumMix(arr)
